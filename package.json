{
  "name": "unnamed-project",
  "version": "0.0.1-alpha.1",
  "description": "TODO: Write project description",
  "license": "MIT-0",
  "private": true,
  "keywords": [],
  "author": {
    "name": "TODO: Add author(s') name(s), URL(s), etc.",
    "url": "https://github.com/github"
  },
  "homepage": "https://github.com/github/TODO#readme",
  "bugs": {
    "url": "https://github.com/github/TODO/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/TODO.git"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/app.js",
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build-all": "npm run all -- build:dev build",
    "build:EXP": "npx tsc --build tsconfig.json",
    "fullbuild": "npm run all -- type-check build:files:scripts build:files:dev build:files:app-dev build:files:dist",
    "build": "npm run all -- type-check-only:app type-check-only:scripts build:files:scripts build:files:dist",
    "build:dev": "npm run all -- type-check-only:scripts build:files:dev build:files:app-dev",
    "build:files:dist": "node build-dev/scripts/project/build.js --dist",
    "build:files:dev": "node build-dev/scripts/project/build.js --dev",
    "build:files:app-dev": "npx tsc --build config/ts/targets/tsconfig.app.json",
    "build:files:scripts": "npx tsc --build config/ts/targets/tsconfig.scripts.json",
    "build:dist": "npm run build:scripts && node build-dev/scripts/project/build.cjs",
    "build:scripts": "node scripts/project/build_scripts.js",
    "clean": "npm run build:scripts && node build-dev/scripts/project/clean.cjs",
    "clean-build": "npm run all -- clean build",
    "clean-fullbuild": "npm run all -- clean fullbuild",
    "watch": "npm run build:files:scripts && npm run all -- --parallel watch:**",
    "watch:files:dist": "node build-dev/scripts/project/watch.js",
    "watch:types": "npx tsc -b tsconfig.json --watch --preserveWatchOutput",
    "clean-build-watch": "npm run all -- clean build:dev build watch",
    "dev-server": "npm run build && npm run all -- --parallel watch:build dev-server-express",
    "dev-server-express": "node build-dev/scripts/project/server_dev.js",
    "dev-server-python": "env python3 -m http.server --directory dist 7878",
    "check": "npm run all -- --sequential check:src check:scripts lint format-check",
    "check-only:app": "npm run _check-only:do -- lint-only:src type-check-only:src",
    "check-only:test": "npm run _check-only:do -- lint-only:test type-check-only:test",
    "check-only:scripts": "npm run _check-only:do -- lint-only:scripts type-check-only:scripts",
    "_check-only:do": "npm run all -- --sequential --continue-on-error",
    "lint": "npx eslint --config config/eslint.config.js",
    "lint-only:app": "npm run lint src",
    "lint-only:test": "npm run lint test",
    "lint-only:scripts": "npm run lint scripts",
    "format-check": "npx prettier --config config/.prettierrc --check package.json scripts config test src",
    "format-check-only:app": "npx prettier --config config/.prettierrc --check src",
    "format-check-only:test": "npx prettier --config config/.prettierrc --check test",
    "format-check-only:config": "npx prettier --config config/.prettierrc --check config",
    "format-check-only:scripts": "npx prettier --config config/.prettierrc --check scripts",
    "format": "npx prettier --config config/.prettierrc --write package.json scripts config test src",
    "format-only:app": "npx prettier --config config/.prettierrc --write src",
    "format-only:test": "npx prettier --config config/.prettierrc --write test",
    "format-only:config": "npx prettier --config config/.prettierrc --write config",
    "format-only:scripts": "npx prettier --config config/.prettierrc --write scripts",
    "type-check": "npx tsc --build tsconfig.json",
    "type-check-only:app": "npm run all -- --sequential --continue-on-error _type-check-only:do:app _type-check-only:do:workers",
    "type-check-only:test": "npx tsc --noEmit --project config/ts/targets/tsconfig.test.json",
    "type-check-only:scripts": "npx tsc --noEmit --project config/ts/targets/tsconfig.scripts.json",
    "_type-check-only:do:app": "npx tsc --noEmit --project config/ts/targets/tsconfig.app.json",
    "_type-check-only:do:workers": "npx tsc --noEmit --project config/ts/targets/tsconfig.app_workers.json",
    "test": "npm run all -- --sequential type-check-only:test _test:do",
    "test-coverage": "npm run all -- --sequential type-check-only:test _test-coverage:do",
    "_test:do": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config config/jest.config.cjs",
    "_test-coverage:do": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config config/jest.config.cjs --coverage",
    "ctags:regen-all": ". scripts/project/ctags/gen.sh --all",
    "ctags:regen-deps": ". scripts/project/ctags/gen.sh --deps",
    "ctags:regen-project": ". scripts/project/ctags/gen.sh --project",
    "all": "npx npm-run-all --npm-path npm"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tanaris": "0.0.1-alpha.5",
    "wouter": "3.7.1"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@jest/globals": "30.2.0",
    "@swc/core": "1.13.19",
    "@swc/jest": "0.2.39",
    "@tailwindcss/forms": "0.5.10",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/express": "5.0.3",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "10.4.21",
    "esbuild": "0.25.10",
    "eslint": "9.36.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-react": "7.37.5",
    "express": "5.1.0",
    "globals": "16.4.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "postcss-import": "16.1.1",
    "postcss-load-config": "6.0.1",
    "prettier": "3.6.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1"
  }
}
